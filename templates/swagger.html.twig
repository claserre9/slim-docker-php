<!-- HTML for static distribution bundle build -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Swagger UI</title>
    <link rel="stylesheet" type="text/css" href="{{ domain }}/dist/swagger-ui.css" />
    <link rel="stylesheet" type="text/css" href="{{ domain }}/dist/index.css" />
    <link rel="icon" type="image/png" href="{{ domain }}/dist/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="{{ domain }}/dist/favicon-16x16.png" sizes="16x16" />
</head>

<body>
<div id="swagger-ui"></div>
<script src="{{ domain }}/dist/swagger-ui-bundle.js" charset="UTF-8"> </script>
<script src="{{ domain }}/dist/swagger-ui-standalone-preset.js" charset="UTF-8"> </script>
<script charset="UTF-8">
    window.onload = function() {

        window.ui = SwaggerUIBundle({
            url: "{{ domain }}/openapi.json",
            dom_id: '#swagger-ui',
            deepLinking: true,
            presets: [
                SwaggerUIBundle.presets.apis,
                SwaggerUIStandalonePreset
            ],
            plugins: [
                SwaggerUIBundle.plugins.DownloadUrl
            ],
            layout: "StandaloneLayout"
        });

    };

</script>
</body>
</html>
